<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>AuthServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.AuthServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountInformationServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.AccountInformationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountCreatServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.AccountCreatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.DeleteAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeInformationServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.TypeInformationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeCreatServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.TypeCreatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransactionCreatServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.TransactionCreatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteTransactionServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.DeleteTransactionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InformationServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.InformationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransactionToCategoryInsertServlet</servlet-name>
        <servlet-class>ru.pavlytskaya.web.TransactionToCategoryInsertServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AuthServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountInformationServlet</servlet-name>
        <url-pattern>/accountInformation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountCreatServlet</servlet-name>
        <url-pattern>/accountCreat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <url-pattern>/deleteAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeInformationServlet</servlet-name>
        <url-pattern>/type</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeCreatServlet</servlet-name>
        <url-pattern>/typeCreat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransactionCreatServlet</servlet-name>
        <url-pattern>/transactionCreat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteTransactionServlet</servlet-name>
        <url-pattern>/deleteTransaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InformationServlet</servlet-name>
        <url-pattern>/information</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransactionToCategoryInsertServlet</servlet-name>
        <url-pattern>/transactionToCategory</url-pattern>
    </servlet-mapping>

</web-app>